OBJS = uimain.o tokenizer.o history.o


all:test

#Pass all objects listed at top to the file
test: $(OBJS)
	cc -o test $(OBJS)

#All obj files that are built from c files that include the .h files
$(OBJS): tokenizer.h history.h


#Deletes all the files generated by compilation
clean:
	rm -f *o test

run:test
	./test
